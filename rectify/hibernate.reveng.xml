<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="rectify" match-name=".*"/>
  <table-filter match-catalog="rectify" match-name="configuration"/>
  <table-filter match-catalog="rectify" match-name="kb_db_op"/>
  <table-filter match-catalog="rectify" match-name="kb_db_op_parts"/>
  <table-filter match-catalog="rectify" match-name="kb_http_request"/>
  <table-filter match-catalog="rectify" match-name="kb_http_request_parts"/>
  <table-filter match-catalog="rectify" match-name="kb_http_response"/>
  <table-filter match-catalog="rectify" match-name="kb_http_response_parts"/>
  <table-filter match-catalog="rectify" match-name="log_db_operation"/>
  <table-filter match-catalog="rectify" match-name="log_db_sub_operation"/>
  <table-filter match-catalog="rectify" match-name="log_http_request"/>
  <table-filter match-catalog="rectify" match-name="log_http_response"/>
  <table-filter match-catalog="rectify" match-name="rectify_log"/>
</hibernate-reverse-engineering>